{
  "name": "damnit-frontend",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "lint:prettier": "pnpm -r lint:prettier",
    "lint:eslint": "pnpm -r lint:eslint",
    "lint:tsc": "pnpm -r lint:tsc",
    "dev:app": "pnpm --filter @damnit-frontend/app dev",
    "build:app": "pnpm --filter @damnit-frontend/app build",
    "preview:app": "pnpm --filter @damnit-frontend/app preview",
    "dev:site": "pnpm --filter @damnit-frontend/site dev",
    "build:site": "pnpm --filter @damnit-frontend/site build",
    "preview:site": "pnpm --filter @damnit-frontend/site preview",
    "dev:demo": "pnpm --filter @damnit-frontend/demo dev",
    "build:demo": "pnpm --filter @damnit-frontend/demo build",
    "preview:demo": "pnpm --filter @damnit-frontend/demo preview",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@damnit-frontend/config": "workspace:*",
